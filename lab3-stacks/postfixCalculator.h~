#include <iostream>
#include <stack>

using namespace std;

class PostfixCalculator{
 public:
  void pushNum(int n);
  void add();
  void subtract();
  void multiply();
  void divide();
  void unaryNeg();
  int getTopValue();
  void readString(string& str);
 private:
  stack<int> s;
  void removeTopValue();
};
